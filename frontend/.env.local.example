# Frontend Environment Variables
# Copy this file to .env.local and update with your local values

# API Configuration
# The API base URL (used if proxy is not configured)
# Default: /api/v1 (relative, uses vite proxy)
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Application Configuration
VITE_APP_TITLE=Rozvrhovac
VITE_APP_DESCRIPTION=School Timetable Management System

# Development Configuration
# Set to 'true' to enable debug logging
VITE_DEBUG=false

# Feature Flags (optional)
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_ERROR_TRACKING=false
